{
  "name": "@strong-config/node",
  "version": "0.2.43",
  "description": "SOPS-based secret management tool for Node.js",
  "repository": "git@github.com:strong-config/node.git",
  "homepage": "https://strong-config.dev",
  "bugs": "https://github.com/strong-config/node/issues",
  "author": "Brickblock Engineering <dev@brickblock.io>",
  "license": "MIT",
  "main": "lib/core/index.js",
  "types": "lib/core/index.d.ts",
  "files": [
    "/bin",
    "/lib",
    "/scripts/postinstall.js",
    "oclif.manifest.json"
  ],
  "bin": {
    "strong-config": "./bin/run"
  },
  "oclif": {
    "commands": "./lib/cli",
    "bin": "strong-config",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-not-found",
      "@oclif/plugin-autocomplete"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 7,
      "message": "<%= config.name %> update available from <%= chalk.greenBright(config.version) %> to <%= chalk.greenBright(latest) %>."
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:clean": "rimraf lib",
    "dev": "tsc --watch -p tsconfig.build.json",
    "dev:importkey": "gpg --import --quiet example/pgp/example-keypair.pgp",
    "lint": "yarn lint:ts && yarn lint:scripts && yarn lint:json && yarn lint:yaml && yarn lint:markdown",
    "lint:json": "eslint . --fix --ext .json",
    "lint:markdown": "markdownlint **/*.md --ignore node_modules --ignore CHANGELOG.md",
    "lint:scripts": "npx scriptlint --fix",
    "lint:ts": "eslint . --fix --ext .ts",
    "lint:yaml": "eslint . --ext .yml,.yaml",
    "postinstall": "node scripts/postinstall.js",
    "postpack": "rimraf oclif.manifest.json",
    "prepack": "yarn build:clean && yarn build && oclif-dev manifest",
    "release": "npx standard-version",
    "report:health": "ts-node --transpile-only scripts/healthcheck.ts",
    "report:todo": "leasot '**/*.ts' --ignore 'node_modules/**/*','lib/**/*','.git/**/*' || true",
    "start": "yarn dev",
    "test": "jest",
    "test:cli": "yarn test:cli:check && yarn test:cli:decrypt && yarn test:cli:encrypt && yarn test:cli:validate && yarn test:cli:generateTypes",
    "test:cli:check": "./bin/run check example/development.yaml --config-root example",
    "test:cli:decrypt": "yarn dev:importkey && ./bin/run decrypt example/development.yaml tmp.yml && cat tmp.yml && rimraf tmp.yml",
    "test:cli:encrypt": "yarn dev:importkey && ./bin/run encrypt -p pgp -k 2E9644A658379349EFB77E895351CE7FC0AC6E94 example/unencrypted.yml tmp.yml && cat tmp.yml && rimraf tmp.yml",
    "test:cli:generateTypes": "rimraf example/config.d.ts && ./bin/run generate-types --config-root example && cat example/config.d.ts",
    "test:cli:validate": "./bin/run validate example/development.yaml --config-root example",
    "test:core": "yarn test:core:load:es6 && yarn test:core:load:commonjs && yarn test:core:validate",
    "test:core:load": "yarn test:core:load:es6",
    "test:core:load:commonjs": "yarn build && yarn dev:importkey && cross-env NODE_ENV=development time node src/integration-tests/load-commonjs.js",
    "test:core:load:es6": "yarn dev:importkey && cross-env NODE_ENV=development time ts-node --transpile-only src/integration-tests/load-es6.ts",
    "test:core:validate": "cross-env NODE_ENV=development time ts-node --transpile-only src/integration-tests/validate.ts"
  },
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/plugin-autocomplete": "^0.2.0",
    "@oclif/plugin-help": "^3.1.0",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "ajv": "^6.12.2",
    "debug": "^4.1.1",
    "execa": "^3.4.0",
    "fast-glob": "^3.2.2",
    "fs-extra": "^9.0.0",
    "js-yaml": "^3.13.1",
    "json-schema-to-typescript": "^8.0.0",
    "match-all": "^1.2.5",
    "node-fetch": "^2.6.0",
    "ora": "^4.0.4",
    "tslib": "^2.0.0",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "@brickblock/eslint-config-base-ts": "^1.0.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@oclif/dev-cli": "^1.22.2",
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^9.0.1",
    "@types/glob": "^7.1.2",
    "@types/jest": "^26.0.0",
    "@types/js-yaml": "^3.12.4",
    "@types/node": "^14.0.13",
    "@types/node-fetch": "^2.5.7",
    "@types/ramda": "^0.27.6",
    "@types/shelljs": "^0.8.8",
    "@types/std-mocks": "^1.0.0",
    "@types/which": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "commitlint": "^8.3.5",
    "cross-env": "^7.0.2",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-tsdoc": "^0.2.5",
    "eslint-plugin-unicorn": "^20.1.0",
    "eslint-plugin-yaml": "^0.1.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-extended": "^0.11.5",
    "leasot": "^11.0.0",
    "lint-staged": "^10.2.6",
    "markdownlint-cli": "^0.23.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ramda": "^0.27.0",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "stdout-stderr": "^0.1.13",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "resolutions": {
    "acorn": ">=7.1.1",
    "lodash": ">=4.7.12",
    "minimatch": ">=3.0.2",
    "tunnel-agent": ">=0.6.0"
  }
}
