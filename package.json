{
  "name": "@strong-config/node",
  "version": "2.0.0",
  "description": "Simple & Secure Config Management for Node.js",
  "repository": "git@github.com:strong-config/node.git",
  "homepage": "https://strong-config.dev",
  "bugs": "https://github.com/strong-config/node/issues",
  "author": "Brickblock Engineering <dev@brickblock.io>",
  "license": "MIT",
  "main": "lib/core/index.js",
  "types": "lib/core/index.d.ts",
  "files": [
    "/bin",
    "/lib",
    "/scripts/postinstall.mjs",
    "oclif.manifest.json"
  ],
  "bin": {
    "strong-config": "./bin/run"
  },
  "engines": {
    "node": ">= 16"
  },
  "oclif": {
    "commands": "./lib/cli",
    "bin": "strong-config",
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 7,
      "message": "<%= config.name %> update available from <%= chalk.greenBright(config.version) %> to <%= chalk.greenBright(latest) %>."
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:clean": "rimraf lib",
    "dev": "tsc --watch -p tsconfig.build.json",
    "dev:importkey": "gpg --import --quiet example/pgp/example-keypair.pgp",
    "format": "prettier --write src/*.ts",
    "lint": "yarn lint:ts && yarn lint:scripts && yarn lint:json && yarn lint:deps && yarn lint:yaml && yarn lint:markdown",
    "lint:deps": "npx npm-check -i '\"{leasot,lint-staged,@commitlint/*,@oclif/plugin-autocomplete,@oclif/plugin-help,@oclif/plugin-not-found,@oclif/plugin-warn-if-update-available,@strong-config/node,execa,ora,tslib,chalk}\"'",
    "lint:json": "eslint . --fix --ext .json",
    "lint:markdown": "markdownlint **/*.md --ignore node_modules --ignore CHANGELOG.md --ignore blackbox",
    "lint:scripts": "npx scriptlint --fix",
    "lint:ts": "eslint . --fix --ext .ts",
    "lint:yaml": "eslint . --ext .yml,.yaml",
    "postinstall": "node scripts/postinstall.mjs; [ -d \".husky\" ] && husky install || true",
    "postpack": "rimraf oclif.manifest.json",
    "prepack": "yarn build:clean && yarn build && npx oclif manifest",
    "release": "npx standard-version",
    "report:health": "node scripts/healthcheck.mjs",
    "report:todo": "leasot '**/*.ts' --ignore 'node_modules/**/*','lib/**/*','.git/**/*','blackbox' || true",
    "start": "yarn dev",
    "test": "jest",
    "test:cli": "yarn test:cli:check && yarn test:cli:decrypt && yarn test:cli:encrypt && yarn test:cli:validate && yarn test:cli:generateTypes",
    "test:cli:check": "./bin/run check example/development.yaml --config-root example",
    "test:cli:decrypt": "yarn dev:importkey && ./bin/run decrypt example/development.yaml tmp.yml && cat tmp.yml && rimraf tmp.yml",
    "test:cli:encrypt": "yarn dev:importkey && ./bin/run encrypt -p pgp -k E263F7743276FB9C209078DF59971A99C34BF6C1 example/unencrypted.yml tmp.yml && cat tmp.yml && rimraf tmp.yml",
    "test:cli:generateTypes": "rimraf example/config.d.ts && ./bin/run generate-types --config-root example && cat example/config.d.ts",
    "test:cli:validate": "./bin/run validate example/development.yaml --config-root example",
    "test:core": "yarn test:core:load:es6 && yarn test:core:load:commonjs && yarn test:core:validate",
    "test:core:load": "yarn test:core:load:es6",
    "test:core:load:commonjs": "yarn build && yarn dev:importkey && cross-env NODE_ENV=development time node src/integration-tests/load-commonjs.js",
    "test:core:load:es6": "yarn dev:importkey && cross-env NODE_ENV=development time ts-node --transpile-only src/integration-tests/load-es6.ts",
    "test:core:validate": "cross-env NODE_ENV=development time ts-node --transpile-only src/integration-tests/validate.ts",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@oclif/core": "^2.11.8",
    "@oclif/plugin-autocomplete": "^2.3.6",
    "@oclif/plugin-help": "^5.2.17",
    "@oclif/plugin-not-found": "^2.3.1",
    "@oclif/plugin-warn-if-update-available": "^2.0.4",
    "ajv": "^8.12.0",
    "debug": "^4.3.1",
    "execa": "^5",
    "fast-glob": "^3.2.5",
    "fs-extra": "^11.1.1",
    "js-yaml": "^4.0.0",
    "json-schema-to-typescript": "^13.0.2",
    "match-all": "^1.2.6",
    "node-fetch": "^3.1.1",
    "ora": "^5",
    "ramda": "^0.29.0",
    "tslib": "^2.6.2",
    "which": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.3",
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.4",
    "@types/js-yaml": "^4.0.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^20.5.3",
    "@types/node-fetch": "^2.5.8",
    "@types/ramda": "^0.29.3",
    "@types/shelljs": "^0.8.8",
    "@types/std-mocks": "^1.0.0",
    "@types/which": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.2",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jest-formatting": "^3.0.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-sonarjs": "^0.21.0",
    "eslint-plugin-tsdoc": "^0.2.11",
    "eslint-plugin-unicorn": "^48.0.1",
    "eslint-plugin-yaml": "^0.5.0",
    "husky": "^8.0.1",
    "jest": "^29.6.3",
    "jest-extended": "^4.0.1",
    "json-schema": "^0.4.0",
    "leasot": "^13.2.0",
    "lint-staged": "^14.0.1",
    "markdownlint-cli": "^0.35.0",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "shelljs": "^0.8.4",
    "stdout-stderr": "^0.1.13",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "volta": {
    "node": "16.20.2"
  }
}
